<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="299.0" prefWidth="509.0" style="-fx-background-color: #FFFFFF;" stylesheets="@../recursos/css/main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SelecionarPecaModal">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefWidth="509.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="txtPesquisar" layoutX="14.0" layoutY="14.0" onKeyPressed="#txtPesquisar_keyPressed" prefHeight="29.0" prefWidth="482.0" promptText="Pesquisar peça . . . " />
            <TableView fx:id="tabelaPecas" layoutX="14.0" layoutY="53.0" prefHeight="194.0" prefWidth="482.0">
               <columns>
                  <TableColumn prefWidth="37.0" text="ID" />
                  <TableColumn minWidth="0.0" prefWidth="339.0" text="Peça" />
                  <TableColumn minWidth="0.0" prefWidth="104.0" text="Preço" />
               </columns>
            </TableView>
            <Button fx:id="btnSelecionar" layoutX="347.0" layoutY="259.0" mnemonicParsing="false" onAction="#btnSelecionar_pressed" prefHeight="29.0" prefWidth="148.0" text="Selecionar" />
            <ProgressIndicator fx:id="progresso" layoutX="465.0" layoutY="19.0" prefHeight="20.0" prefWidth="20.0" style="-fx-progress-color: -cor-borda-controle;" />
         </children>
      </AnchorPane>
   </children>
</VBox>
